{
  "name": "mfp",
  "private": true,
  "version": "2.1.132",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:gui": "vitest --ui",
    "postinstall": "graphql-codegen",
    "build": "tsc && vite build",
    "gen": "graphql-codegen",
    "gen-watch": "graphql-codegen -w",
    "lint": "eslint src/**",
    "preview": "vite preview",
    "format": "prettier  \"**/*.{ts,tsx,css}\" --write",
    "convert:items": "ts-node --esm src/__scripts__/createItemList.ts",
    "convert:recipes": "ts-node --esm src/__scripts__/createRecipeList.ts",
    "bump": "npm version patch --no-git-tag-version && git add package.json"
  },
  "pre-commit": [
    "bump"
  ],
  "dependencies": {
    "@apollo/client": "^3.8.2",
    "@dnd-kit/core": "^6.0.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.15",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@use-gesture/react": "^10.2.20",
    "aws-sdk": "^2.1350.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cva": "npm:class-variance-authority",
    "fast-csv": "^4.3.6",
    "graphql": "16.6.0",
    "javascript-time-ago": "^2.5.9",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.277.0",
    "node-fzf": "^0.11.0",
    "react": "^18.2.0",
    "react-charts": "^3.0.0-beta.54",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.0",
    "react-split-pane": "^0.1.92",
    "react-spring": "^9.5.5",
    "react-spritesheet": "^1.0.1",
    "reactflow": "^11.1.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@parcel/watcher": "^2.3.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/inline-style-prefixer": "^5.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.28.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.18",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "tailwindcss": "^3.2.1",
